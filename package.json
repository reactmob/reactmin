{
    "name": "reactmin",
    "version": "0.0.1",
    "private": true,
    "description": "dos-react-admin created by Rekit.",
    "keywords": [
        "rekit"
    ],
    "babel": {
        "presets": [
            "es2015",
            "react",
            "babel-preset-stage-0",
            "babel-preset-stage-1",
            "babel-preset-stage-2",
            "babel-preset-stage-3"
        ],
        "plugins": [
            "react-hot-loader/babel",
            "syntax-dynamic-import",
            "transform-decorators-legacy",
            "lodash",
            [
                "module-resolver",
                {
                    "alias": {
                        "src": "./src"
                    }
                }
            ]
        ],
        "env": {
            "dev": {
                "plugins": []
            },
            "test": {
                "plugins": [
                    "istanbul"
                ]
            }
        }
    },
    "nyc": {
        "include": [
            "index.js",
            "src/**/*.js",
            "tools/lib/*.js"
        ],
        "exclude": [
            "tools/**/templates",
            ".tmp"
        ],
        "reporter": [
            "lcov",
            "text-summary"
        ],
        "cache": false,
        "sourceMap": false,
        "instrument": true
    },
    "rekit": {
        "version": "2.0.3",
        "devPort": 6075,
        "portalPort": 6076,
        "buildPort": 6077,
        "plugins": [
            "redux-saga"
        ],
        "css": "sass"
    },
    "scripts": {
        "start": "node ./tools/server.js",
        "build": "node ./tools/build.js",
        "test": "node ./tools/run_test.js"
    },
    "devDependencies": {
        "@uifabric/utilities": "^4.9.0",
        "argparse": "^1.0.7",
        "axios": "^0.16.2",
        "babel-core": "^6.25.0",
        "babel-eslint": "^7.2.3",
        "babel-loader": "^7.0.0",
        "babel-plugin-istanbul": "^4.1.4",
        "babel-plugin-lodash": "^3.2.11",
        "babel-plugin-module-resolver": "^2.7.1",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-polyfill": "^6.23.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "babel-preset-stage-1": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "babel-preset-stage-3": "^6.24.1",
        "babel-register": "^6.24.1",
        "chai": "^4.0.2",
        "css-loader": "^0.28.4",
        "enzyme": "^2.8.2",
        "eslint": "^4.0.0",
        "eslint-config-airbnb": "^15.0.1",
        "eslint-import-resolver-babel-module": "^3.0.0",
        "eslint-plugin-import": "^2.3.0",
        "eslint-plugin-jsx-a11y": "^5.0.3",
        "eslint-plugin-react": "^7.1.0",
        "estraverse": "^4.2.0",
        "estraverse-fb": "^1.3.1",
        "express": "^4.15.3",
        "express-history-api-fallback": "^2.2.1",
        "file-loader": "^0.11.2",
        "immutable": "^3.8.1",
        "js-logger": "^1.3.0",
        "jsdom": "^11.0.0",
        "lodash": "^4.17.4",
        "lodash-webpack-plugin": "^0.11.4",
        "mocha": "^3.4.2",
        "mocha-webpack": "^0.7.0",
        "nock": "^9.0.13",
        "node-sass": "^4.5.3",
        "normalizr": "^3.2.3",
        "normalizr-immutable": "^0.0.3",
        "npm-run": "^4.1.2",
        "nyc": "^11.0.2",
        "office-ui-fabric-react": "^4.20.0",
        "path-to-regexp": "^1.7.0",
        "photon": "https://github.com/connors/photon.git",
        "prop-types": "^15.5.10",
        "react": "^15.6.1",
        "react-addons-perf": "^15.4.2",
        "react-addons-test-utils": "^15.6.0",
        "react-dom": "^15.6.1",
        "react-fa": "^4.2.0",
        "react-hot-loader": "^3.0.0-beta.6",
        "react-paginate": "^4.4.3",
        "react-redux": "^5.0.5",
        "react-router": "^4.1.1",
        "react-router-dom": "^4.1.1",
        "react-router-redux": "next",
        "react-split-pane": "^0.1.64",
        "reapop": "^1.1.1",
        "reapop-theme-wybo": "^0.4.1",
        "redux": "^3.7.0",
        "redux-form": "^7.0.2",
        "redux-logger": "^3.0.6",
        "redux-mock-store": "^1.2.3",
        "redux-saga": "^0.15.4",
        "redux-thunk": "^2.2.0",
        "rekit-core": "^2.0.0",
        "rekit-plugin-redux-saga": "^1.0.0",
        "rekit-portal": "^2.0.0",
        "reselect": "^3.0.1",
        "sass-loader": "^6.0.6",
        "sinon": "^2.3.5",
        "style-loader": "^0.18.2",
        "throttle-debounce": "^1.0.1",
        "tree-model": "^1.0.6",
        "url-loader": "^0.5.9",
        "webpack": "^3.0.0",
        "webpack-bundle-analyzer": "^2.8.2",
        "webpack-dev-middleware": "^1.10.2",
        "webpack-hot-middleware": "^2.18.0",
        "webpack-node-externals": "^1.6.0",
        "whatwg-fetch": "^2.0.3",
        "with-query": "^1.0.2"
    },
    "dependencies": {
        "react-tabs": "^1.1.0",
        "reactmob-filter": "^0.2.0"
    }
}
